<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TrainEverywhere.RegistrationPageNext">
	<ContentPage.Content>
		<StackLayout>
			<Entry Placeholder="Height" Keyboard="Numeric"/>
			<Entry Placeholder="Weight" Keyboard="Numeric"/>
			<Entry Placeholder="Chest" Keyboard="Numeric"/>
			<Entry Placeholder="Arms" Keyboard="Numeric"/>
			<Entry Placeholder="Shoulders" Keyboard="Numeric"/>
			<Entry Placeholder="Waist" Keyboard="Numeric"/>
			<Entry Placeholder="Calves" Keyboard="Numeric"/>
			<Entry Placeholder="Quads" Keyboard="Numeric"/>

			<Button Margin="0,20,0,0" Text="Sign up" TextColor="White" BackgroundColor="Coral" HorizontalOptions="EndAndExpand"/>
		</StackLayout>
	</ContentPage.Content>
</ContentPage>